select distinct sp1_0.id,sp1_0.label,sp1_0.currency,sp1_0.tp,sp1_0.days,sp1_0.maturity,sp1_0.swap_pt_bid,sp1_0.swap_pt_ask,sp1_0.data_set,sp1_0.swap_pt_mid,sp1_0.date,rc1_0.curve_spread_mid,rc1_0.curve_spread_bid,rc1_0.curve_spread_ask,rc1_0.market_quote_mid,rc1_0.market_quote_bid,rc1_0.market_quote_ask from tx_md_swap_points sp1_0,tx_md_rate_curve rc1_0 where sp1_0.currency=rc1_0.currency and sp1_0.label=rc1_0.label and sp1_0.tp=rc1_0.tp and sp1_0.maturity=rc1_0.maturity_label and (lower(sp1_0.data_set) like ? escape '') and (lower(rc1_0.data_set) like ? escape '') and (lower(sp1_0.currency) like ? escape '') and (lower(rc1_0.currency) like ? escape '') and (lower(sp1_0.label) like ? escape '') and (lower(rc1_0.label) like ? escape '') offset ? rows fetch first ? rows only